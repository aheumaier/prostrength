<div class="nested-fields">
  <table class="table">
    <thead>
      <tr></tr>
    </thead>
    <tbody >
      <td>
        <div class="mb-3">

          <%= f.label :exercise_id,{ class: "form-label"} %>
          <%= f.text_field :exercise_id, list: 'excercises_autocomplete', data: { action: 'input->autocompletion#autocomplete'}, class: 'form-control'%>
          <datalist id="excercises_autocomplete">
            <% Exercise.all.each do |exercise|%>
              <option value="<%= exercise.title %>" ></option>

            <%end%>
          </datalist>
        </div>

      </td>
      <td>
        <div class="mb-3">
          <%= f.label :series, { class: "form-label"} %>
          <%= f.text_field :series, {class: 'form-control' } %>
        </div>
      </td>
      <td>
        <div class="mb-3">
          <%= f.label :grip, { class: "form-label"} %>
          <%= f.collection_select :grip, @grips, :to_s, :to_s, { promt: "Select a grip", include_blank: false,}, { class: 'form-select'} %>
        </div>
      </td>
      <td>
        <div class="mb-3">
          <%= f.label :repetition,{ class: "form-label"} %>
          <%= f.text_field :repetition, {class: 'form-control' } %>
        </div>
      </td>
      <td>
        <div class="mb-3">
          <%= f.label :tempo,{ class: "form-label"} %>
          <%= f.text_field :tempo, {class: 'form-control' } %>
        </div>
      </td>
      <td>
        <div class="mb-3">
          <%= f.label :pause,{ class: "form-label"} %>
          <%= f.text_field :pause, {class: 'form-control' } %>
        </div>
      </td>
      <td>
        <%= link_to_remove_association '<i class="bi bi-dash-circle-fill"></i>'.html_safe, f %>
      </td>
    </tbody>
  </table>
</div>
